{
  "name": "@deepracticex/nodespec-cli",
  "version": "0.1.0",
  "description": "NodeSpec CLI - AI-friendly Node.js project scaffolding tool",
  "type": "module",
  "bin": {
    "nodespec": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup && tsx scripts/copy-mirror.ts",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "test": "pnpm build && vitest run",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@deepracticex/nodespec-core": "workspace:*",
    "@deepracticex/nodespec-mirror": "workspace:*",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "execa": "^9.5.2",
    "globby": "^14.0.2",
    "js-yaml": "^4.1.0",
    "ora": "^8.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@deepracticex/config-preset": "workspace:*",
    "@deepracticex/vitest-cucumber": "^1.2.0",
    "@types/js-yaml": "^4.0.9"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "cli",
    "scaffolding",
    "nodespec",
    "monorepo",
    "typescript",
    "ai-development",
    "deepractice"
  ],
  "author": "Deepractice",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deepractice/NodeSpec"
  }
}
